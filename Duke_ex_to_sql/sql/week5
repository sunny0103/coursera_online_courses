question2.
select brand ,size,colorfrom skuinfowhere brand='polo fas' and (size='XXL' or color='black');

question3.
select str.store, str.city, str.state,df.months,df.years
from(select store, saledate, extract(month from saledate) as months, extract(year from saledate) as years
from trnsact) as df join strinfo str
on df.store=str.store
GROUP BY str.city,str.state,str.store,df.months,df.years
HAVING COUNT(DISTINCT df.saledate)=11;

